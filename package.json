{
  "name": "mental-health-check",
  "version": "0.1.0",
  "engines": {
    "node": "16.14.0"
  },
  "description": "Mental Health Test",
  "main": "server/server.js",
  "scripts": {
    "start": "node -r dotenv/config server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i --legacy-peer-deps",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/priyansh32/mh-check.git"
  },
  "keywords": [
    "mental-health",
    "depression",
    "anxiety",
    "schizophrenia",
    "post-traumatic-stress"
  ],
  "author": "nitrotap",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/priyansh32/mh-check/issues"
  },
  "homepage": "https://github.com/priyansh32/mh-check#readme",
  "devDependencies": {
    "concurrently": "^7.2.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
